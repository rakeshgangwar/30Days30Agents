# Image Generation Assistant Environment Configuration
# Copy this file to .env and fill in your values

# API Keys (at least one required)
# OpenAI API Key for DALL-E image generation
OPENAI_API_KEY=your_openai_api_key_here

# Fal.ai API Key for Fal.ai models (fast-sdxl, flux, etc.)
FAL_KEY=your_fal_api_key_here

# Optional Configuration (defaults shown)

# Storage directories
IMAGE_STORAGE_DIR=generated_images
METADATA_STORAGE_DIR=metadata

# Storage limits
MAX_STORAGE_MB=1000
CLEANUP_AFTER_DAYS=30

# API rate limiting (requests per minute)
RATE_LIMIT_RPM=50

# Request timeout in seconds
REQUEST_TIMEOUT=60

# Default image generation parameters
DEFAULT_MODEL=dall-e-3
DEFAULT_SIZE=1024x1024
DEFAULT_QUALITY=standard
DEFAULT_STYLE=vivid

# LLM configuration for prompt enhancement
LLM_MODEL=openai:gpt-4o-mini
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=500

# Fal.ai specific configuration
FAL_DEFAULT_MODEL=fal-ai/fast-sdxl
FAL_ENABLE_QUEUE_MONITORING=true
FAL_MAX_QUEUE_WAIT_TIME=300
FAL_ENABLE_STREAMING=true 