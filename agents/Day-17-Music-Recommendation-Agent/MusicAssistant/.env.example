# This is an example .env file for the application.
# Copy this file to .env and fill in the required values.
# Make sure to rename this file to .env before running the application.

# OpenRouter API key
# Replace with your actual OpenRouter API key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Spotify API credentials
# Replace with your actual Spotify API credentials
# You can obtain these by creating an application in the Spotify Developer Dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SPOTIFY_REDIRECT_URI=http://127.0.0.1:8080/callback