# Required API Keys for the Travel Agent
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (Required)
# Used for the language model that powers the agent
OPENAI_API_KEY=your_openai_api_key_here

# Google Maps API Key (Optional but recommended)
# Used for transportation, restaurant, and hotel information
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Tavily API Key (Optional but recommended)
# Used for attractions and local tips
TAVILY_API_KEY=your_tavily_api_key_here

# OpenWeatherMap API Key (Optional)
# Used for weather forecasts
OPENWEATHERMAP_API_KEY=your_openweathermap_api_key_here
